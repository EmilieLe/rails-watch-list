<h1>All Lists</h1>

<ul>
  <% @lists.each do |list| %>
    <li>
      <%= link_to list.name, list_path(list) %>
      <%= link_to 'Add Bookmark', new_list_bookmark_path(list), class: 'btn btn-primary' %>
    </li>
  <% end %>
</ul>

<%= link_to 'Create New List', new_list_path, class: 'btn btn-secondary' %>
